{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "isolatedModules": false,
    "allowJs": false,
    "strict": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "lib": ["ES2020"],
    "outDir": "dist"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "include": ["src"]
}


